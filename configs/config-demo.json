{
  "main": {
    "addr": "127.0.0.1:8080",
    "apiAllowedIPs": [
      "*"
    ]
  },
  "cron": {
    "commit-job-worker": "1m",
    "duration-worker": "3m",
    "footprint-worker": "5m"
  },
  "auth": {
    "jwts": {
      "max-age": "2000h"
    }
  },
  "metric-store": {
    "checkpoints": {
      "interval": "12h"
    },
    "retention-in-memory": "48h"
  }
}